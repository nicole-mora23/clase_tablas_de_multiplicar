#include <stdio.h>
int main(){
int i, j, m,k;

printf("Tablas utilizando while\n\n");

i=1;
while(i<=12){
    printf("Tabla del %d\n",i);
    printf("------------\n");
    j=1;
    
    while(j<=12){
        if (j==6){
            j++;
        }
        m=i*j;
        printf("%d * %d = %d\n",i,j,m);
        j++;
    }
  
   i++;

    }
    //break;
    k=m*24;
}
